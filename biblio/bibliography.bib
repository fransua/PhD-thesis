keywords = {Animal,Animal: physiology,Animals,Biological Evolution,Cichlids,Cichlids: physiology,Female,Male,Regression Analysis,Sexual Behavior,Sperm Motility,Sperm Motility: physiology,Spermatozoa,Spermatozoa: physiology,protamine\_paper},
