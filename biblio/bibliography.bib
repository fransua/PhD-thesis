journal = {Nature},
keywords = {Arthritis,Bipolar Disorder,Bipolar Disorder: genetics,Case-Control Studies,Chromosomes,Coronary Artery Disease,Coronary Artery Disease: genetics,Crohn Disease,Crohn Disease: genetics,Diabetes Mellitus,Diabetes Mellitus: genetics,Gene Frequency,Genetic Markers,Genetic Markers: genetics,Genetic Predisposition to Disease,Genetic Predisposition to Disease: genetics,Genetics,Genome,Geography,Great Britain,Human,Human: genetics,Humans,Hypertension,Hypertension: genetics,Population,Rheumatoid,Rheumatoid: genetics,affymetrix500k,disease,evolution,genome,gwa},
