issn = {0737-4038},
journal = {Molecular biology and evolution},
keywords = {Alleles,Amino Acids,Amino Acids: genetics,Bayes Theorem,Computer Simulation,Genes,Genetic,Humans,Major Histocompatibility Complex,Major Histocompatibility Complex: genetics,Selection,Viral,evolution,paml,positiveselection,printed,protamine\_paper},
