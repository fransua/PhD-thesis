issn = {1558-5646},
journal = {Evolution},
keywords = {Animals,Ants,Ants: chemistry,Ants: genetics,Argentina,Behavior, Animal,Behavior, Animal: physiology,Evolution,Female,Genetic Structures,Genetic Variation,Genetics, Population,Hydrocarbons,Hydrocarbons: analysis,Male,Social Behavior},
