file = {:home/garamonfok/Ubuntu One/mendeley/1991/Carr\'{e}-Eus\`{e}be et al. - 1991 - Processing of the precursor of protamine P2 in mouse. Peptide mapping and N-terminal sequence analysis of intermediates.pdf:pdf},
