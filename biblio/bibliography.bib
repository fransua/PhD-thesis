issn = {0362-2525},
journal = {Journal of morphology},
keywords = {Animals,Biological,Biological Evolution,Male,Models,Phylogeny,Rodentia,Rodentia: anatomy \& histology,Species Specificity,Sperm Head,Sperm Head: ultrastructure,Sperm Tail,Sperm Tail: ultrastructure,Spermatozoa,Spermatozoa: ultrastructure},
month = sep,
number = {3},
pages = {271--90},
pmid = {16037955},
title = {{Evolution of the spermatozoon in muroid rodents.}},
