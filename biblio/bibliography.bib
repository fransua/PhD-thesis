issn = {1553-7404},
journal = {PLoS genetics},
keywords = {Amino Acid Substitution,Animals,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila melanogaster,Drosophila melanogaster: genetics,Evolution,Genetic,Molecular,Multigene Family,Mutation,Phylogeny,Selection},
month = feb,
