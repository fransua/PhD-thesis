issn = {1088-9051},
journal = {Genome research},
keywords = {Animals,Computational Biology,Computational Biology: methods,Drosophila,Drosophila: genetics,Evolution,Female,Gene Duplication,Genes,Insect,Male,Molecular,Molecular Sequence Data,Proteomics,Proteomics: methods,Seminal Plasma Proteins,Seminal Plasma Proteins: genetics,protamine\_paper},
