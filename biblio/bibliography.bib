journal = {Genetics},
keywords = {Animals,Dogs,Evolution,Gene Dosage,Gene Duplication,Genetic,Humans,Likelihood Functions,Macaca mulatta,Macaca mulatta: genetics,Mammals,Mammals: genetics,Mice,Molecular,Multigene Family,Pan troglodytes,Pan troglodytes: genetics,Primates,Primates: genetics,Rats,Selection},
month = nov,
number = {3},
pages = {1941--9},
pmid = {17947411},
title = {{Accelerated rate of gene gain and loss in primates.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17947411 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2147951\&tool=pmcentrez\&rendertype=abstract},
volume = {177},
