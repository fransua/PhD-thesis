issn = {0027-5107},
journal = {Mutation research},
keywords = {Chromosome Aberrations,Cohort Studies,Europe,Genetic,Genetic Markers,Humans,Neoplasms,Neoplasms: epidemiology,Neoplasms: genetics,Neoplasms: metabolism,Polymorphism,Risk Assessment,Sister Chromatid Exchange,Xenobiotics,Xenobiotics: metabolism,aberations,cancer},
