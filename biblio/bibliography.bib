journal = {Molecular biology and evolution},
keywords = {Animals,Evolution,Genitalia,Humans,Male,Male: chemistry,Mice,Molecular,Prostate,Prostate: chemistry,Proteomics,Semen,Semen: chemistry,Seminal Plasma Proteins,Seminal Plasma Proteins: analysis,Seminal Plasma Proteins: genetics,Seminal Vesicles,Seminal Vesicles: chemistry,protamine\_paper},
