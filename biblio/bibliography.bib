@misc{TarragaJoaquinMedinaIgnacioHuertas-CepasJaimeSerraFrancoisArbizaLeonardoGabaldonToniDopazoJoaquin,
abstract = {Phylemon 2.0 is the second release of the suite of web-tools for molecular evolution, phylogenetics and phylogenomics previously published in 2007 (NAR 35:W38-42). The new web server integrates a suite of more than 30 different tools for phylogenetic and evolutionary analyses. It is conceived as a natural response to the increasing demand of data analysis in many different practical courses on phylogeny as well as labs of experimental scientists willing to add a molecular evolution and phylogenetics insight into their research using the more classical tools and the new developments in the area. In this context we launch Phylemon 2.0 with new tools and utilities such as TrimAl, ReadAl, Lagan, Mlagan, jModelTest and HyPhy tools to test for molecular adaptation considering synonymous rate variation across sites. All these tools are integrated in its main blocks: utilities, alignment, phylogenetic analysis and evolutionary tests. Phylemon 2.0 has been completely re-engineered and now it is based on AJAX's Web 2.0 technology features. This has facilitated the design of a new interface that allows asynchronous use, as well as projects, jobs and user management. The tools and utilities included in Phylemon 2.0 range from file format conversion (ReadAl), multiple sequence alignments (ClustalW, Muscle, Lagan and Mlagan), alignment edition (TrimAl), basic algorithms of distance and parsimony methods (Phylip programs), to more complex maximum likelihood (PhyML, jModelTest) and Bayesian methods (MrBayes) for phylogenetic reconstruction. Moreover, Phylemon 2.0 provides evolutionary tests to search for the best evolutionary model explaining DNA (ModelTest) and protein (Protest) sequences data under the HyPhy environment; relative rate tests (RRTree); tests of positive selection by means of branch, site and branch-site models of PAML (CodeML); the site-wise likelihood ratio test (SLR) for detection of sites under positive and negative selection, and the new HyPhy developments for adaptive evolution considering synonymous rate (dS) variation across sites. All tools are integrated in a shared user friendly web-environment with infile examples to run and the help files in all programs. Phylemon 2.0 can manage hundreds of independent input and output files for a phylogenomic analysis. In this case, fully automatic and basic phylogenomic analyses can be executed by defining pipelines through a modular design environment implemented in a Java applet. Phylemon 2.0 is a useful integrated web-server implementing classical and more advanced statistical evolutionary analyses for phylogenetic, phylogenomics and evolutionary hypothesis testing.},
address = {Valencia, Spain},
author = {Tarraga, Joaquin and Medina, Ignacio and Huerta-Cepas, Jaime and Serra, Fran\c{c}ois and Arbiza, Leonardo and Gabald\'{o}n, Toni and Dopazo, Joaqu\'{\i}n and Dopazo, Hern\'{a}n},
keywords = {poster},
mendeley-tags = {poster},
pages = {February 13th},
title = {{Phylemon 2.0: New resources in a suite of web tools for molecular evolution, phylogenetics and phylogenomics}},
url = {http://cbbl.imim.es:8080/RNB/jornadas},
year = {2008}
}
