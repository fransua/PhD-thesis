number = {Suppl 1},
pages = {S4+},
pmid = {20158875},
title = {{Projection of gene-protein networks to the functional space of the proteome and its application to analysis of organism complexity.}},
url = {http://dx.doi.org/10.1186/1471-2164-11-S1-S4 http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2822532\&tool=pmcentrez\&rendertype=abstract},
volume = {11 Suppl 1},
