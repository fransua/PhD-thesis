journal = {Genome research},
keywords = {Animals,Conserved Sequence,Drosophila Proteins,Drosophila Proteins: genetics,Drosophila melanogaster,Drosophila melanogaster: genetics,Drosophila melanogaster: metabolism,Evolution,Genetic Variation,Genome,Insect,Molecular,Nucleic Acid,Sequence Homology,Synteny,Synteny: genetics,adaptiveevolution,drosophila,ortholog\_paralog,printed,rate},
mendeley-tags = {adaptiveevolution,drosophila,ortholog\_paralog,printed,rate},
month = dec,
number = {12},
pages = {1837--49},
pmid = {17989258},
publisher = {Cold Spring Harbor Lab},
title = {{Evolutionary rate analyses of orthologs and paralogs from 12 Drosophila genomes.}},
