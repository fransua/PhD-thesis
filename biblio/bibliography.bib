issn = {0737-4038},
journal = {Molecular biology and evolution},
keywords = {Adaptation,Animals,Drosophila melanogaster,Drosophila melanogaster: genetics,Drosophila melanogaster: metabolism,Evolution,Metabolic Networks and Pathways,Metabolic Networks and Pathways: genetics,Molecular,Molecular Sequence Data,Physiological,Physiological: genetics,adaptiveevolution,annotation,drosophila,evolution,metabolism,printed,project\_funcanalysis},
