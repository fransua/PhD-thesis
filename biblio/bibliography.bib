journal = {BMC evolutionary biology},
keywords = {Amino Acid Sequence,Bacterial,Chaperonin 60,Chaperonin 60: genetics,Chlamydia,Chlamydia: classification,Chlamydia: genetics,Evolution,Gene Duplication,Genes,Molecular,Phylogeny,Sequence Alignment,coevolution,evolution,tools},
