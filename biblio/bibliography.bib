month = jul,
number = {7149},
pages = {83--86},
publisher = {Nature Publishing Group},
title = {{Intronic microRNA precursors that bypass Drosha processing}},
url = {http://www.nature.com/nature/journal/vaop/ncurrent/full/nature05983.html},
volume = {448},
year = {2007}
}
