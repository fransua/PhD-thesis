journal = {BMC evolutionary biology},
keywords = {Animals,Collagen Type I,Collagen Type I: chemistry,Collagen Type I: genetics,Fertilization,Genetic,Humans,Mammals,Mammals: genetics,Mammals: physiology,Phylogeny,Proteins,Proteins: genetics,Reproduction,Selection,disease,evolution,reproductive\_protein},
