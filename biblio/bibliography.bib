pmid = {15383915},
publisher = {Springer},
title = {{A maximum likelihood method for detecting functional divergence at individual codon sites, with application to gene family evolution.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/15383915},
volume = {59},
