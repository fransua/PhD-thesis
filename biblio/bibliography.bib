keywords = {Alternative Splicing,Alternative Splicing: genetics,Animals,Base Sequence,Conserved Sequence,DNA,DNA: genetics,Evolution,Exons,Exons: genetics,Genetic,Genome,Genomics,Human,Human: genetics,Humans,Introns,Introns: genetics,Messenger,Messenger: genetics,Mice,Molecular,RNA,Reading Frames,Selection,colocalization,evolution,exon,expression,mammals,printed,project\_coexcoev},
mendeley-tags = {colocalization,evolution,exon,expression,mammals,printed,project\_coexcoev},
