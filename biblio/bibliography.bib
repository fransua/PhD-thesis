number = {3},
pages = {934--9},
pmid = {18199838},
publisher = {National Acad Sciences},
title = {{High-confidence prediction of global interactomes based on genome-wide coevolutionary networks.}},
url = {http://www.pnas.org/content/105/3/934.short http://www.pubmedcentral.nih.gov/articlerender.fcgi?artid=2242690\&tool=pmcentrez\&rendertype=abstract},
volume = {105},
