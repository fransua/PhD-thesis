keywords = {Animals,Chimera,Chimera: genetics,Chromatin,Chromatin: metabolism,Chromatin: ultrastructure,DNA Damage,Electron,Embryo Loss,Embryo Loss: etiology,Embryo Loss: genetics,Embryo Loss: metabolism,Female,Genotype,Inbred C57BL,Male,Mice,Microscopy,Mutant Strains,Pregnancy,Protamines,Protamines: genetics,Protamines: metabolism,Scanning,Spermatozoa,Spermatozoa: metabolism,Spermatozoa: ultrastructure,protamine\_paper},
