author = {Singer, Gregory A. C and Lloyd, Andrew T and Huminiecki, Lukasz B and Wolfe, Kenneth H},
doi = {10.1093/molbev/msi062},
file = {:home/garamonfok/Ubuntu One/mendeley/2005/Singer et al. - 2005 - Clusters of co-expressed genes in mammalian genomes are conserved by natural selection.pdf:pdf},
