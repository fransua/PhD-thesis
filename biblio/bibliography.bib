keywords = {Animals,Bayes Theorem,Computer Simulation,Fishes,Gene Frequency,Genetic,Genetic Variation,Genetics,Genome,Genome: genetics,Humans,Likelihood Functions,Microsatellite Repeats,Models,Polymorphism,Population,Single Nucleotide,Statistics as Topic,evolution,fst,microsatellite,population\_genomics,printed},
mendeley-tags = {evolution,fst,microsatellite,population\_genomics,printed},
month = oct,
number = {2},
pages = {861--73},
pmid = {17660541},
title = {{Empirical Bayes inference of pairwise F(ST) and its distribution in the genome.}},
