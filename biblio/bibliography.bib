number = {2},
pages = {539--50},
pmid = {17135334},
title = {{Mechanisms and rates of birth and death of dispersed duplicated genes during the evolution of a multigene family in diploid and tetraploid wheats.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/17135334},
volume = {24},
year = {2007}
}
